Rebirth = 1 

BountySkillSection:NewToggle("  Loop  Farm  Rebirth ", "", function(state)
    if state then
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "BountySkill Hub ";
            Text = "Auto Farm Money";
            Icon = "http://www.roblox.com/asset/?id=10629794985"})
            Duration = 5; 

        getgenv().AutoMoney = true
        if getgenv().AutoMoney == true then
            repeat wait()
				local Rebirth
				Rebirth = 1 
				game:GetService("ReplicatedStorage"):FindFirstChild("functions-shared/network@GlobalFunctions"):FindFirstChild("s:rebirth"):FireServer(Rebirth)


	until getgenv().AutoMoney == false
end
    else
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "BountySkill Hub ";
            Text = "Stoped !!";
            Icon = "http://www.roblox.com/asset/?id=10629794985"})
            Duration = 5; 

       getgenv().AutoMoney = false

    end
end)

Section:NewTextBox("Rebirth", "", function(txt)
	Rebirth = txt
end)


GoldenDevilsSection:NewButton("Copy Url Discord Server GD", "", function()
	game:GetService("StarterGui"):SetCore("SendNotification", { 
		Title = "BountySkill Hub ";
		Text = "Done!";
		Icon = "http://www.roblox.com/asset/?id=10629794985"})
		Duration = 5; 

	setclipboard("discord.gg/GoldenDevils")

end)



