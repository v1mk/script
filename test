-- Scripts:
 
local Player = function(Ev) -- الفنكشن ذا يجيب الاعب من اسمه حتى لو كتبت حرفين من اسمه
				if Ev == "" then
					return nil
				elseif Ev == "random" then
					return game:GetService("Players"):GetPlayers()[math.random(1, #game:GetService("Players"):GetPlayers())]
				else
					for _, v in pairs(game:GetService("Players"):GetPlayers()) do
						if v.Name:lower():sub(1, #Ev) == Ev:lower() or v.DisplayName:lower():sub(1, #Ev) == Ev then
							return v
						end
					end
				end
			end
 
TextBox.FocusLost:Connect(function()
 
				local TargetV = Player(TextBox.Text) -- يجيب اسم الاعب من الtxtbox
				if TargetV ~= game:GetService("Players").LocalPlayer then -- اذا المستهدف صار انت يسوي سكب
					TextBox.Text = TargetV.Name -- مجرد يحط اسم الاعب
				end
			end)   
 
TextButton.MouseButton1Down:Connect(function()
local TargetV = Player(TextBox.Text) -- تعريف المستهدم من الtxtbox
local plr = game.Players.LocalPlayer
local hum = plr.Character.HumanoidRootPart

game:GetService("TweenService"):Create(hum,TweenInfo.new(1),{plr.CFrame = TargetV.Character.HumanoidRootPart.CFrame)} -- ينتقل للاعب
end)

    TextButton_2.MouseButton1Down:Connect(function()
        ScreenGui:Destroy()
    end)


