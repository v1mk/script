        TabButton.Name = "TabButton"
        TabButton.Parent = TabsFrame
        TabButton.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
        TabButton.BackgroundTransparency = 0.80
        TabButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
