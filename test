Script:AddToggle({
	Name = "ÈÚíÏ ãä ÞÊá",
	Default = false,
	Callback = function(x)
    if x == true then
        RunService:BindToRenderStep("Reach", 0 , function()
            local tool = plr.Character and plr.Character:FindFirstChildOfClass("Tool")
            if tool and tool:FindFirstChild("Handle") then
                local p = game.Players:GetPlayers()
                for _ = 2, #p do local v = p[i].Character
                    if v and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and plr:DistanceFromCharacter(v.HumanoidRootPart.Position) <= 30 then
                        for _,v in next, v:GetChildren() do
                            if v:IsA("BasePart") then
                                firetouchinterest(tool.Handle,v,0)
                                firetouchinterest(tool.Handle,v,1)
                            end
                        end
                    end
                end
            end
        end)
    else
        RunService:UnbindFromRenderStep("Reach")
    end
end})

