Esp:AddToggle({Name = "ÇáÞÇÊá", Default = false, Callback = function(x)
    getgenv().MurderEspName = x
    while getgenv().MurderEspName do
        wait()
        pcall(function()
            for i, v in pairs(folder:GetChildren()) do
                if v:IsA("BillboardGui") and players[tostring(v.Name)] then
                    if players[tostring(v.Name)].Character:FindFirstChild("Knife") or players[tostring(v.Name)].Backpack:FindFirstChild("Knife")  then
                        if getgenv().MurderEspName then
                            v.Enabled = true
                        else
                            v.Enabled = false
                        end
                    end
                end
            end
        end)
    end
end})

Esp:AddToggle({Name = "ÇáÔÑØí", Default = false, Callback = function(x)
    getgenv().SheriffEspName = x
    while getgenv().SheriffEspName do
        wait()
        pcall(function()
            for i, v in pairs(folder:GetChildren()) do
                if v:IsA("BillboardGui") and players[tostring(v.Name)] then
                    if players[tostring(v.Name)].Character:FindFirstChild("Gun") or players[tostring(v.Name)].Backpack:FindFirstChild("Gun")  then
                        if getgenv().SheriffEspName then
                            v.Enabled = true
                        else
                            v.Enabled = false
                        end
                    end
                end
            end
        end)
    end
end})

Esp:AddToggle({Name = "Çáßá", Default = false, Callback = function(x)
    getgenv().AllEspName = x
    for i, v in pairs(folder:GetChildren()) do
        if v:IsA("BillboardGui") and players[tostring(v.Name)] then
            if getgenv().AllEspName then
                v.Enabled = true
            else
                v.Enabled = false
            end
        end
    end
end})

Esp:AddToggle({Name = "ÇáÓáÇÍ", Default = false, Callback = function(x)
    getgenv().EspGunName = x
    if getgenv().EspGunName then
        while wait(1) do
            task.wait(0.5)
            pcall(function()
                for i,v in pairs(Workspace:GetDescendants()) do
                    if v.Name == "GunDrop" and not v:FindFirstChild("EspGun") then
                        local bill= Instance.new("BillboardGui",v)
                        bill.Name = "EspGun"
                        bill.AlwaysOnTop = true
                        bill.Size = UDim2.fromOffset(200,50)
                        bill.ExtentsOffset = Vector3.new(0,3,0)
                        bill.Enabled = true
    
                        local textlabel = Instance.new("TextLabel",bill)
                        textlabel.TextSize = 20
                        textlabel.Text = "åäÇ ÇáÓáÇÍ"
                        textlabel.Font = Enum.Font.SourceSans
                        textlabel.BackgroundTransparency = 1
                        textlabel.Size = UDim2.fromScale(1,1)
                        textlabel.TextColor3 = Color3.fromRGB(255,255,255)
                        textlabel.TextStrokeTransparency = 0
                        textlabel.TextYAlignment = Enum.TextYAlignment.Bottom
                        textlabel.ZIndex = 0
                        repeat
                            billboard.Adornee = v
                        until getgenv().EspGunName == false
                    end
                end
            end)
            if not getgenv().EspGunName then
                bill:Destroy()
                textlabel:Destroy()
                break
            end
        end
    end
end})

--// Target

Target:AddSection({Name = "ÇáãÓÊåÏÝ ÇÎÊíÇÑ"})
Target:AddTextbox({
	Name = "ÇáÇÓã",
	Default = "",
	TextDisappear = true,
	Callback = function(sex)
        for i,v in pairs(game.Players:GetChildren()) do
            if v.Name ~= plr.Name then
                if string.sub(string.lower(v.Name),1,string.len(sex)) == string.lower(sex) or string.sub(string.lower(v.DisplayName),1,string.len(sex)) == string.lower(sex) then
                    Target = v
                    TargetV = game.Players[v.Name]
                    SendNotify("Target : "..TargetV.Name,4)
                    return
                end
            end
        end
        SendNotify("ÏáÎ ßÇáÚÇÏÉ",4)
    end
})

Target:AddButton({Name = "ÚÔæÇÆí ÔÎÕ ÇÎÊíÇÑ" ,Callback = function()
    local random = players:GetPlayers()[math.random(1, #players:GetPlayers())]
    TargetV = game.Players[random.Name]
    SendNotify("Target : "..TargetV.Name,4)
end})

Target:AddSection({Name = ""})
Target:AddSection({Name = "ÚÇãÉ ÎíÇÑÇÊ"})

Target:AddButton({Name = "ÇäÊÞÇá" ,Callback = function()
    plr.Character.HumanoidRootPart.CFrame = TargetV.Character.HumanoidRootPart.CFrame
end})

Target:AddButton({Name = "ÝáíäÞ" ,Callback = function()
    FlingPlayer(TargetV.Name)
end})

Target:AddToggle({Name = "ãÔÇåÏÉ", Default = false, Callback = function(x)
	if x == true then
		Workspace.Camera.CameraSubject = TargetV.Character.Humanoid
	elseif x == false then
	    Workspace.Camera.CameraSubject = plr.Character.Humanoid
	end	
end})

Target:AddSection({Name = ""})
Target:AddSection({Name = "ÇáÞÏÑÇÊ"})

Target:AddButton({Name = "ÇáÇÚÈ ÚäÏ ÝÎ ÑÓÈæä" ,Callback = function()
    pcall(function()
        ReplicatedStorage:WaitForChild("TrapSystem"):WaitForChild("PlaceTrap"):InvokeServer(unpack({
            [1] = TargetV.Character.HumanoidRootPart.CFrame
        }))
    end)
end})

Target:AddToggle({Name = "ÇáÇÚÈ ÚäÏ ÝÎ ÑÓÈæä ÓÈÇã", Default = false, Callback = function(x)
    getgenv().LoopTarp = x
    while getgenv().LoopTarp == true do
        wait()
        pcall(function()
            ReplicatedStorage:WaitForChild("TrapSystem"):WaitForChild("PlaceTrap"):InvokeServer(unpack({
                [1] = TargetV.Character.HumanoidRootPart.CFrame
            }))
        end)
    end
end})


Target:AddSection({Name = ""})
Target:AddSection({Name = "ÞÇÊá ßäÊ ÇÐÇ"})

Target:AddButton({Name = "ÞÊá" ,Callback = function()
    local Knife = plr.Backpack:FindFirstChild("Knife") or plr.Character:FindFirstChild("Knife")
    GetKnife()
    if Knife then
        if Knife:IsA("Tool") then
            if TargetV.Character ~= nil then
                VirtualUser:CaptureController()
                VirtualUser:ClickButton1(Vector2.new())
                firetouchinterest(TargetV.Character.Head, Knife.Handle, 0)
                firetouchinterest(TargetV.Character.Head, Knife.Handle, 1)
            end
        end
    end

end})

--// Settings

Settings:AddSection({Name = "ÇáÞíã"})

Settings:AddButton({Name = "FPS ÈæÓÊ" ,Callback = function()
    local decalsyeeted = true
    local g = game
    local w = g.Workspace
    local l = g.Lighting
    local t = w.Terrain
    t.WaterWaveSize = 0
    t.WaterWaveSpeed = 0
    t.WaterReflectance = 0
    t.WaterTransparency = 0
    l.GlobalShadows = false
    l.FogEnd = 9e9
    l.Brightness = 0
    settings().Rendering.QualityLevel = "Level01"
    for i,v in pairs(g:GetDescendants()) do
        if v:IsA("Part") or v:IsA("Union") or v:IsA("MeshPart") then
            v.Material = "Plastic"
    v.Reflectance = 0
    elseif v:IsA("Decal") and decalsyeeted then 
    v.Transparency = 1
    elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then 
    v.Lifetime = NumberRange.new(0)
        end
    end
end})

Settings:AddSection({Name = ""})
Settings:AddSection({Name = "ÇáÓíÑÝÑ"})

Settings:AddButton({Name = "ááÓíÑÝÑ ÏÎæá ÇÚÇÏÉ" ,Callback = function()
    game:GetService("TeleportService"):Teleport(game.PlaceId)
end})


