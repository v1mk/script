Player:AddToggle({
	Name = "ãÓØÍÉ ÔÇÔÉ",
	Default = false,
	Callback = function(x)
        if x == true then
            getgenv().Resolution = {
                [".gg/87JE7dmM"] = 0.65
            }
        else
            getgenv().Resolution = {
                [".gg/87JE7dmM"] = 1
            }
        end
        local Camera = workspace.CurrentCamera
        if getgenv().gg_87JE7dmM == nil then
            game:GetService("RunService").RenderStepped:Connect(function()
                Camera.CFrame = Camera.CFrame * CFrame.new(0, 0, 0, 1, 0, 0, 0, getgenv().Resolution[".gg/87JE7dmM"], 0, 0, 0, 1)
            end)
        end
        getgenv().gg_87JE7dmM = "Illness"
    end
})



Player:AddSection({Name = ""})
Player:AddSection({Name = "ÇáãÖÇÏÇÊ"})

Player:AddButton({Name = "ÇáÝáíäÞ ãÖÇÏ", Callback = function()
    if getgenv().Antifling == false then
        getgenv().Antifling = true
        RunService:BindToRenderStep("anti fling", 0 , function()
            for _, CoPlayer in pairs(players:GetChildren()) do
                if CoPlayer ~= plr and CoPlayer.Character then
                    for _,part in pairs(CoPlayer.Character:GetChildren()) do
                        if part:IsA("BasePart") or part:IsA("Part") or part.Name == "HumanoidRootPart" then
                            part.CanCollide = false
                            part.CustomPhysicalProperties = PhysicalProperties.new(0,0,0,0,0)
                            part.Velocity = VE(0,0,0)
                            part.RotVelocity = VE(0,0,0)
                        end
                    end
                end
            end
        end)
        SendNotify("ÇáÊÝÚíá Êã",4)
    else
        SendNotify("íÇÛÈí ÇäÊ ãÝÚáå",4)
    end
end})

Player:AddToggle({Name = "ÇáÓæÏÇÁ ÇáÔÇÔÉ ãÖÇÏ", Default = false, Callback = function(x)
    if x == true then
        plr.PlayerGui.SpawnFade.Enabled = false
        plr.PlayerGui.CameraFade.Enabled = false
    else
        plr.PlayerGui.SpawnFade.Enabled = true
        plr.PlayerGui.CameraFade.Enabled = true
    end
end})

