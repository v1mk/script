--[ functions ]--
local function SendNotify(text,time)
    Notify({
        Description = text,
        Title = "Atomic Hub",
        Duration = time,
    })
end

local function getRootPart()
   local character = plr.Character or plr.CharacterAdded:Wait()
   return character:WaitForChild("HumanoidRootPart")
end

local function teleport(cframe)
   local rootPart = getRootPart()
   
   connections["Heartbeat"] = RunService.Heartbeat:Connect(function()
       local oldVelocity = rootPart.Velocity
       
       rootPart.Velocity = Vector3.new(0, -550, 0)
       RunService.RenderStepped:Wait()
       rootPart.Velocity = oldVelocity

       task.wait(.4)
       connections["Heartbeat"]:Disconnect()
   end)
   
   task.wait(.2)
   rootPart.CFrame = cframe
end

local function IsAlive(Player)
	for _, v in pairs(GetPlayerData) do
		if Player.Name == _ then
			if not v.Killed and not v.Dead then
				return true
			else
				return false
			end
		end
	end
end

function XrayOn(obj)
    for _,v in pairs(obj:GetChildren()) do
        if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
            v.LocalTransparencyModifier = 0.75
        end
        XrayOn(v)
    end
end

function XrayOff(obj)
    for _,v in pairs(obj:GetChildren()) do
        if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
            v.LocalTransparencyModifier = 0
        end 
        XrayOff(v)
    end
end

local function GetGun()
    for _,v in pairs(plr.Backpack:GetChildren()) do
        if v:IsA"Tool" and v.Name == "Gun" then
            v.Parent = plr.Character
        end
    end
end

local function GetKnife()
    for _,v in pairs(plr.Backpack:GetChildren()) do
        if v:IsA"Tool" and v.Name == "Knife" then
            v.Parent = plr.Character
        end
    end
end
    
local function CreateHighlightForPlayers()
	for _, v in pairs(players:GetChildren()) do
		if v ~= plr and v.Character and not v.Character:FindFirstChild("Highlight") then
			local Highlight = Instance.new("Highlight", v.Character)
            Highlight.FillTransparency  = 2
            Highlight.OutlineTransparency = 2
		end
	end
end

local function CreateHighlightForGun()
	for _, v in pairs(Workspace:GetChildren()) do
		if v.Name == "GunDrop" and not v:FindFirstChild("Highlight") then
			local Highlight = Instance.new("Highlight", v)
            Highlight.FillTransparency  = 2
            Highlight.OutlineTransparency = 2
		end
	end
end

local function DestroyHighlightFromPlayers()
	for _, v in pairs(players:GetChildren()) do
		if v ~= plr and v.Character and v.Character:FindFirstChild("Highlight") then
			v.Character:FindFirstChild("Highlight"):Destroy()
		end
	end
end

local function DestroyHighlightFromGun()
	for _, v in pairs(Workspace:GetChildren()) do
		if v.Name == "GunDrop" and v:FindFirstChild("Highlight") then
			v:FindFirstChild("Highlight"):Destroy()
		end
	end
end

local function UpdateHighlightsForPlayers()
	for _, v in pairs(players:GetChildren()) do
		if v ~= plr and v.Character and v.Character:FindFirstChild("Highlight") then
			Highlight = v.Character:FindFirstChild("Highlight")
			if v.Name == Sheriff and IsAlive(v) then
                if getgenv().SheriffEsp == true then
                    Highlight.FillColor = Color3.fromRGB(0, 0, 225)
                    Highlight.FillTransparency  = getgenv().EspTransparency
                    Highlight.OutlineColor = Color3.fromRGB(0, 0, 225)
                else
                    Highlight.FillTransparency  = 2
                    Highlight.OutlineTransparency = 2
                end
                if getgenv().EspLine == true then
                    Highlight.OutlineTransparency = 0
                else
                    Highlight.OutlineTransparency = 2
                end
            elseif v.Name == Murder and IsAlive(v) then
                if getgenv().MurderEsp == true then
                    Highlight.FillColor = Color3.fromRGB(225, 0, 0)
                    Highlight.FillTransparency  = getgenv().EspTransparency
                    Highlight.OutlineColor = Color3.fromRGB(225, 0, 0)
                else
                    Highlight.FillTransparency  = 2
                    Highlight.OutlineTransparency = 2
                end
                if getgenv().EspLine == true then
                    Highlight.OutlineTransparency = 0
                else
                    Highlight.OutlineTransparency = 2
                end
			elseif v.Name == Hero and IsAlive(v) and not IsAlive(game.Players[Sheriff]) then
                if getgenv().SheriffEsp == true then
                    Highlight.FillColor = Color3.fromRGB(255, 250, 0)
                    Highlight.FillTransparency  = getgenv().EspTransparency
                    Highlight.OutlineColor = Color3.fromRGB(255, 250, 0)
                else
                    Highlight.FillTransparency  = 2
                    Highlight.OutlineTransparency = 2
                end
                if getgenv().EspLine == true then
                    Highlight.OutlineTransparency = 0
                else
                    Highlight.OutlineTransparency = 2
                end
            else
                if getgenv().AllEsp == true then
                    Highlight.FillColor = Color3.fromRGB(0, 225, 0)
                    Highlight.FillTransparency  = getgenv().EspTransparency
                    Highlight.OutlineColor = Color3.fromRGB(0, 225, 0)
                else
                    Highlight.FillTransparency  = 2
                    Highlight.OutlineTransparency = 2
                end
                if getgenv().EspLine == true then
                    Highlight.OutlineTransparency = 0
                else
                    Highlight.OutlineTransparency = 2
                end
			end
		end
	end
end	

local function UpdateHighlightsForGun()
	for _, v in pairs(Workspace:GetChildren()) do
		if v.Name == "GunDrop" then
            Highlight = v:FindFirstChild("Highlight")
            if getgenv().GunEsp == true then
                Highlight.FillColor = Color3.fromRGB(0, 200, 0)
                Highlight.FillTransparency  = getgenv().EspTransparency
            else
                Highlight.FillTransparency  = 2
            end
            if getgenv().EspLine == true then
                Highlight.OutlineTransparency = 0
            else
                Highlight.OutlineTransparency = 2
            end
		end
	end
end

local FlingPlayer = function(s)
    local Targets = {s} -- "All", "Target Name", "arian_was_here"

    local Players = game:GetService("Players")
    local Player = Players.LocalPlayer
    
    local AllBool = false
    
    local GetPlayer = function(Name)
        Name = Name:lower()
        if Name == "all" or Name == "others" then
            AllBool = true
            return
        elseif Name == "random" then
            local GetPlayers = Players:GetPlayers()
            if table.find(GetPlayers,Player) then table.remove(GetPlayers,table.find(GetPlayers,Player)) end
            return GetPlayers[math.random(#GetPlayers)]
        elseif Name ~= "random" and Name ~= "all" and Name ~= "others" then
            for _,x in next, Players:GetPlayers() do
                if x ~= Player then
                    if x.Name:lower():match("^"..Name) then
                        return x;
                    elseif x.DisplayName:lower():match("^"..Name) then
                        return x;
                    end
                end
            end
        else
            return
        end
    end
        
    local Fling = function(TargetPlayer)
        local Character = Player.Character
        local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
        local RootPart = Humanoid and Humanoid.RootPart
    
        local TCharacter = TargetPlayer.Character
        local THumanoid
        local TRootPart
        local THead
        local Accessory
        local Handle
    
        if TCharacter:FindFirstChildOfClass("Humanoid") then
            THumanoid = TCharacter:FindFirstChildOfClass("Humanoid")
        end
        if THumanoid and THumanoid.RootPart then
            TRootPart = THumanoid.RootPart
        end
        if TCharacter:FindFirstChild("Head") then
            THead = TCharacter.Head
        end
        if TCharacter:FindFirstChildOfClass("Accessory") then
            Accessory = TCharacter:FindFirstChildOfClass("Accessory")
        end
        if Accessoy and Accessory:FindFirstChild("Handle") then
            Handle = Accessory.Handle
        end
    
        if Character and Humanoid and RootPart then
            if RootPart.Velocity.Magnitude < 50 then
                getgenv().OldPos = RootPart.CFrame
            end
            if THumanoid and THumanoid.Sit and not AllBool then
                return error(".")
            end
            if THead then
                workspace.CurrentCamera.CameraSubject = THead
            elseif not THead and Handle then
                workspace.CurrentCamera.CameraSubject = Handle
            elseif THumanoid and TRootPart then
                workspace.CurrentCamera.CameraSubject = THumanoid
            end
            if not TCharacter:FindFirstChildWhichIsA("BasePart") then
                return
            end
            
            local FPos = function(BasePart, Pos, Ang)
                RootPart.CFrame = CF(BasePart.Position) * Pos * Ang
                Character:SetPrimaryPartCFrame(CF(BasePart.Position) * Pos * Ang)
                RootPart.Velocity = VE(9e7, 9e7 * 10, 9e7)
                RootPart.RotVelocity = VE(9e8, 9e8, 9e8)
            end
            
            local SFBasePart = function(BasePart)
                local TimeToWait = 2
                local Time = tick()
                local Angle = 0
    
                repeat
                    if RootPart and THumanoid then
                        if BasePart.Velocity.Magnitude < 50 then
                            Angle = Angle + 100
    
                            FPos(BasePart, CF(0, 1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle),0 ,0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(2.25, 1.5, -2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(-2.25, -1.5, 2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(Angle), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, 1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(Angle), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(Angle), 0, 0))
                            task.wait()
                        else
                            FPos(BasePart, CF(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5, -THumanoid.WalkSpeed), CFrame.Angles(0, 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
                            task.wait()
                            
                            FPos(BasePart, CF(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5, -TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(0, 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5, 0), CFrame.Angles(math.rad(90), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5, 0), CFrame.Angles(0, 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5 ,0), CFrame.Angles(math.rad(-90), 0, 0))
                            task.wait()
    
                            FPos(BasePart, CF(0, -1.5, 0), CFrame.Angles(0, 0, 0))
                            task.wait()
                        end
                    else
                        break
                    end
                until BasePart.Velocity.Magnitude > 500 or BasePart.Parent ~= TargetPlayer.Character or TargetPlayer.Parent ~= Players or not TargetPlayer.Character == TCharacter or THumanoid.Sit or Humanoid.Health <= 0 or tick() > Time + TimeToWait
            end
            
            workspace.FallenPartsDestroyHeight = 0/0
            
            local BV = Instance.new("BodyVelocity")
            BV.Name = "EpixVel"
            BV.Parent = RootPart
            BV.Velocity = VE(9e8, 9e8, 9e8)
            BV.MaxForce = VE(1/0, 1/0, 1/0)
            
            Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
            
            if TRootPart and THead then
                if (TRootPart.CFrame.p - THead.CFrame.p).Magnitude > 5 then
                    SFBasePart(THead)
                else
                    SFBasePart(TRootPart)
                end
            elseif TRootPart and not THead then
                SFBasePart(TRootPart)
            elseif not TRootPart and THead then
                SFBasePart(THead)
            elseif not TRootPart and not THead and Accessory and Handle then
                SFBasePart(Handle)
            else
                return error(".")
            end
            
            BV:Destroy()
            Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
            workspace.CurrentCamera.CameraSubject = Humanoid
            
            repeat
                RootPart.CFrame = getgenv().OldPos * CF(0, .5, 0)
                Character:SetPrimaryPartCFrame(getgenv().OldPos * CF(0, .5, 0))
                Humanoid:ChangeState("GettingUp")
                table.foreach(Character:GetChildren(), function(_, x)
                    if x:IsA("BasePart") then
                        x.Velocity, x.RotVelocity = VE(), VE()
                    end
                end)
                task.wait()
            until (RootPart.Position - getgenv().OldPos.p).Magnitude < 25
            workspace.FallenPartsDestroyHeight = getgenv().FPDH
        else
            return error(".")
        end
    end
    
    getgenv().Welcome = true
    if Targets[1] then for _,x in next, Targets do GetPlayer(x) end else return end
    
    if AllBool then
        for _,x in next, Players:GetPlayers() do
            if not table.find(sex, x.Name) then
                if not string.find(ua2,x.UserId) then        
                    Fling(x)
                end
            end
        end
    end
    
    for _,x in next, Targets do
        if GetPlayer(x) and GetPlayer(x) ~= Player then
                local TPlayer = GetPlayer(x)
                if TPlayer then
                    Fling(TPlayer)
                end
        elseif not GetPlayer(x) and not AllBool then
            error(".")
        end
    end
end

--[ esp ]--
RunService.RenderStepped:connect(function()
    if getgenv().EspEnabled == true then
        roles = ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
        for _, v in pairs(roles) do
            if v.Role == "Murderer" then
                Murder = _
            elseif v.Role == 'Sheriff'then
                Sheriff = _
            elseif v.Role == 'Hero'then
                Hero = _
            end
        end
        CreateHighlightForPlayers()
        CreateHighlightForGun()
        UpdateHighlightsForPlayers()
        UpdateHighlightsForGun()
    else
        DestroyHighlightFromPlayers()
        DestroyHighlightFromGun()
    end
end)

--[ esp names ]--

local folder = Instance.new("Folder",game.CoreGui)
folder.Name = "ESP Holder"
	
local function AddBillboard(player)
    local billboard = Instance.new("BillboardGui",folder)
    billboard.Name = player.Name
    billboard.AlwaysOnTop = true
    billboard.Size = UDim2.fromOffset(200,50)
    billboard.ExtentsOffset = Vector3.new(0,3,0)
    billboard.Enabled = false

    local textlabel = Instance.new("TextLabel",billboard)
    textlabel.TextSize = 20
    textlabel.Text = player.Name
    textlabel.Font = Enum.Font.FredokaOne
    textlabel.BackgroundTransparency = 1
    textlabel.Size = UDim2.fromScale(1,1)
    textlabel.TextStrokeTransparency = 2
    textlabel.TextYAlignment = Enum.TextYAlignment.Bottom
    textlabel.ZIndex = 0
    if getgenv().AllEspName then
        billboard.Enabled = true
    end
    repeat
        wait()
        pcall(function()
            billboard.Adornee = player.Character.Head
            if player.Character:FindFirstChild("Knife") or player.Backpack:FindFirstChild("Knife") then
                textlabel.TextColor3 = Color3.fromRGB(255,0,0)
                if not billboard.Enabled and getgenv().MurderEspName then
                    billboard.Enabled = true
                end
            elseif player.Character:FindFirstChild("Gun") or player.Backpack:FindFirstChild("Gun") then
                textlabel.TextColor3 = Color3.fromRGB(0,0,255)
                if not billboard.Enabled and getgenv().SheriffEspName then
                    billboard.Enabled = true
                end
                elseif player.Character:FindFirstChild("Gun") and not IsAlive(players[Sheriff]) then
                textlabel.TextColor3 = Color3.fromRGB(255,255,0)
            else
                textlabel.TextColor3 = Color3.fromRGB(0,255,0)
            end
        end)
    until not player.Parent
end

for _,player in pairs(players:GetPlayers()) do
    if player ~= plr then
        coroutine.wrap(AddBillboard)(player)
    end
end

players.PlayerAdded:Connect(AddBillboard)

players.PlayerRemoving:Connect(function(player)
    folder[player.Name]:Destroy()
end)

