	Name = "ÇáÓáÇÍ ÓÍÈ",
	Callback = function()
    if not plr.Backpack:FindFirstChild("Knife") or plr.Character:FindFirstChild("Knife") then
        local oldpos = plr.Character.HumanoidRootPart.CFrame
        for _,v in pairs(Workspace:GetDescendants()) do
            if v.Name == "GunDrop" and plr.PlayerGui.MainGUI.Game.CashBag.Visible == true then
                plr.Character.HumanoidRootPart.CFrame = v.CFrame * CF(0, 0, 4)
                wait()
                plr.Character.HumanoidRootPart.CFrame = v.CFrame * CF(0, 2, 0)
                wait()
                plr.Character.HumanoidRootPart.CFrame = v.CFrame
                wait()
                plr.Character.HumanoidRootPart.CFrame = v.CFrame * CF(0, 2, 0)
                wait()
            end
        end
        wait(.3)
        plr.Character.HumanoidRootPart.CFrame = oldpos
    end
end})

