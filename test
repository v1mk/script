ToggleButton.BorderSizePixel = 0
ToggleButton.Position = UDim2.new(0.0197142698, 0, 0, 0)
ToggleButton.Size = UDim2.new(0, 254, 0, 33)
ToggleButton.Font = Enum.Font.RobotoCondensed
