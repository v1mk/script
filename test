	Name = "ÇíãÈæÊ",
	Default = Enum.KeyCode.F,
	Hold = false,
	Callback = function()
        for _,a in pairs(plr.Backpack:GetChildren()) do
            if a:IsA"Tool" and a.Name == "Gun" then
                for _,v in pairs(game.Players:GetPlayers()) do
                    if v.Character ~= nil and v.Backpack:FindFirstChild("Knife") or v.Character:FindFirstChild("Knife") then
                        GetGun()
                        wait(0.3)
                        local args = {[1] = 1,[2] = v.Character.HumanoidRootPart.Position,[3] = "AH"}
                        plr.Character.Gun.KnifeServer.ShootGun:InvokeServer(unpack(args))
                    end
                end
            end
        end    
    end
})
